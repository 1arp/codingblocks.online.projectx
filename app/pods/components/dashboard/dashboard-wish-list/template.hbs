{{#each wishlist as |item|}}
  <div class="hover-grey px-5 py-4">
    <div class="row mx-0 align-items-center">
      <div class="col-2">
        <img src="{{item.course.logo}}" alt="">
      </div>
      <div class="col-10">
        <h5 class="bold">{{item.course.title}}</h5>
        <div class="mt-3 rating">
          <span class="pos-rating orange">
            <i class="fas fa-star fa-lg"></i>
          </span>
          <span class="pos-rating orange pl-2">
            <i class="fas fa-star fa-lg"></i>
          </span>
          <span class="pos-rating orange pl-2">
            <i class="fas fa-star fa-lg"></i>
          </span>
          <span class="pos-rating orange pl-2">
            <i class="fas fa-star fa-lg"></i>
          </span>
          <span class="neg-rating pl-2">
            <i class="fas fa-star fa-lg"></i>
          </span>
        </div>
      </div>
    </div>
  </div>
{{/each}}
